{
  "version": 2,
  "builds": [
    {
      "src": "Backend/backend.ts",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "configuration": {
        "installCommand": "npm install",
        "buildCommand": "npm run build",
        "outputDirectory": "dist"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/Backend/backend.ts"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}